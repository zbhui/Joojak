\begin_layout Standard
N-S方程的粘性项满足
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{F}^{v}(\boldsymbol{u},\nabla\boldsymbol{u})=\mathscr{G}:\nabla\boldsymbol{u}
\]

\end_inset


\end_layout

\begin_layout Standard
或写成
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f_{p\alpha}^{v}=g_{\alpha\beta}^{pq}\partial_{\beta}u_{q}
\]

\end_inset


\end_layout

\begin_layout Standard
其中
\begin_inset Formula $\mathscr{G}$
\end_inset

为四阶张量，
\begin_inset Formula $:$
\end_inset

表示张量的缩并，
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underbar{\tau}:\underbar{\sigma}=\text{\sigma}_{i\alpha}\tau_{i\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
将粘性项定义为中间变量
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{G}=\mathscr{G}(\boldsymbol{u}):\nabla\boldsymbol{u}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{t}(J\boldsymbol{u})+\nabla\cdot\left(J(\mathcal{F}^{c}-\mathcal{G}-\boldsymbol{u}\boldsymbol{v}_{g})\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard
第一式乘以
\begin_inset Formula $\mathcal{T}$
\end_inset

，分部积分
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\int_{\kappa}\mathcal{G}:\mathcal{T}\text{d}V & = & \int_{\kappa}(\mathscr{G}:\nabla\boldsymbol{u}):\mathcal{T}\text{d}V\\
 & = & \int_{\kappa}\nabla\boldsymbol{u}:(\mathscr{G}^{\mathit{T}}:\mathcal{T})\text{d}V\\
 & = & \int_{\kappa}\boldsymbol{u}:\nabla\cdot(\mathscr{G}^{\mathit{T}}:\mathcal{T})\text{d}V-\int_{\partial\kappa}\boldsymbol{u}\otimes\boldsymbol{n}:(\mathscr{G}^{\mathit{T}}:\mathcal{T})\text{d}S
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\otimes$
\end_inset

表示张量的并矢，
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{u}\otimes\boldsymbol{v}=u_{\alpha}v_{\beta}
\]

\end_inset


\end_layout

\begin_layout Standard
由于在单元边界，变量是间断的，定义通量:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\hat{\boldsymbol{u}}=\boldsymbol{u}(\boldsymbol{u}^{+},\boldsymbol{u}^{-})
\]

\end_inset


\end_layout

\begin_layout Standard
在分部积分得
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\int_{\kappa}\mathcal{G}:\mathcal{T}\text{d}V & = & \int_{\kappa}(\mathscr{G}:\nabla\boldsymbol{u}):\mathcal{T}\text{d}V+\int_{\partial\kappa}(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n}:(\mathscr{G}^{\mathit{T}}:\mathcal{T})\text{d}S\\
 & = & \int_{\kappa}\mathcal{F}^{v}:\mathcal{T}\text{d}V+\int_{\partial\kappa}\mathscr{G}:(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n}:\mathcal{T}\text{d}S\\
 & = & \int_{\kappa}\mathcal{F}^{v}:\mathcal{T}\text{d}V+\int_{\partial\kappa}\mathcal{F}^{v}(\boldsymbol{u},(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n}):\mathcal{T}\text{d}S
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
第二式乘以
\begin_inset Formula $\boldsymbol{\varphi}$
\end_inset

，分部积分
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\kappa}\text{\ensuremath{\partial}}_{t}\boldsymbol{u}\cdot\boldsymbol{\varphi}\text{d}V+\int_{\partial\kappa}(\hat{\mathcal{F}^{c}}-\hat{\mathcal{F}^{v}})\cdot\boldsymbol{\varphi}\text{d}S-\int_{\kappa}\mathcal{F}^{c}:\nabla\boldsymbol{\phi}\text{d}V+\int_{\kappa}\mathcal{G}:\nabla\boldsymbol{\varphi}\text{d}V=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\hat{\mathcal{F}^{c}}$
\end_inset

和
\begin_inset Formula $\hat{\mathcal{F}^{v}}$
\end_inset

分别为无粘通量和粘性通量。
\end_layout

\begin_layout Standard
为了消去中间变量
\begin_inset Formula $\mathcal{G}$
\end_inset

，令
\begin_inset Formula $\mathcal{T}=\nabla\boldsymbol{\varphi}$
\end_inset

，得
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\kappa}\text{\ensuremath{\partial}}_{t}\boldsymbol{u}\cdot\boldsymbol{\varphi}\text{d}V+\int_{\partial\kappa}(\hat{\mathcal{F}^{c}}-\hat{\mathcal{F}^{v}}\boldsymbol{n})\cdot\boldsymbol{\varphi}\text{d}S-\int_{\kappa}(\mathcal{F}^{c}-\mathcal{F}^{v}):\nabla\boldsymbol{\varphi}\text{d}V+\int_{\partial\kappa}\mathcal{F}^{v}(\boldsymbol{u},(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n}):\nabla\boldsymbol{\varphi}\text{d}S=0
\]

\end_inset


\end_layout

\begin_layout Standard
上式还有三个通量
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\hat{\boldsymbol{u}}=\{\boldsymbol{u}\}=\frac{1}{2}(\boldsymbol{u}^{+}+\boldsymbol{u}^{-})
\]

\end_inset


\end_layout

\begin_layout Standard
则
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n}=\frac{1}{2}(\boldsymbol{u}^{+}-\boldsymbol{u}^{-})\otimes\boldsymbol{n}=-\frac{1}{2}[\boldsymbol{u}]
\]

\end_inset


\end_layout

\begin_layout Standard
记惩罚项
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{P}=\mathcal{F}^{v}(\boldsymbol{u},(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n})=\mathcal{F}^{v}(\boldsymbol{u},\frac{1}{2}[\boldsymbol{u}])
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{P}^{+}(\boldsymbol{u}^{+},\boldsymbol{u}^{-})=\mathcal{F}^{v}(\boldsymbol{u}^{+},(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n})=\mathcal{F}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}])
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{P}^{-}(\boldsymbol{u}^{+},\boldsymbol{u}^{-})=\mathcal{F}^{v}(\boldsymbol{u}^{-},(\hat{\boldsymbol{u}}-\boldsymbol{u})\otimes\boldsymbol{n})=\mathcal{F}^{v}(\boldsymbol{u}^{-},\frac{1}{2}[\boldsymbol{u}])
\]

\end_inset


\end_layout

\begin_layout Standard
粘性通量
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\hat{\mathcal{F}^{v}} & = & \frac{1}{2}(\mathcal{F}^{v}(\boldsymbol{u}^{+},\nabla\boldsymbol{u}^{+})+\mathcal{F}^{v}(\boldsymbol{u}^{-},\nabla\boldsymbol{u}^{-}))-C_{IP}\frac{p^{2}}{h_{f}}\mathscr{G}(\{\boldsymbol{u}\})[\boldsymbol{u}]\\
 & = & \frac{1}{2}(\mathcal{F}^{v+}+\mathcal{F}^{v-})-C_{IP}\frac{p^{2}}{h_{f}}(\mathcal{P}^{+}+\mathcal{P}^{-})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
无粘通量取Lax-Friedrichs通量。
\end_layout

