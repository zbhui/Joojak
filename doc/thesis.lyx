\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{x}=\boldsymbol{x}(\boldsymbol{\xi},t)
\]

\end_inset


\end_layout

\begin_layout Standard
设变换是正则的，并且关于时间
\begin_inset Formula $t$
\end_inset

可微的，变换的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{J}=\frac{\partial\boldsymbol{x}}{\partial\boldsymbol{\xi}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $J=\text{det}\boldsymbol{J}$
\end_inset

，体积微元
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\text{d}v=J\text{d}V\label{eq:volume partial}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
面积微元
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\boldsymbol{n}\text{d}s=J^{-1}\boldsymbol{J}^{\text{T}}\boldsymbol{N}\text{d}S\label{eq:area partial}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
将方程
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:NS eqn"

\end_inset

在
\begin_inset Formula $v(t)$
\end_inset

上积分，并利用散度定理
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{v(t)}\frac{\partial\boldsymbol{U}}{\partial t}\text{d}v+\int_{s(t)}\mathcal{F}\cdot\boldsymbol{n}\text{d}s=0
\]

\end_inset


\end_layout

\begin_layout Standard
利用坐标变换
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{s(t)}\mathcal{F}\cdot\boldsymbol{n}\text{d}s=\int_{S}\mathcal{F}\cdot J\boldsymbol{J}^{-T}\text{d}\boldsymbol{S}=\int_{S}J\boldsymbol{J}^{-1}\mathcal{F}\cdot\text{d}\boldsymbol{S}=\int_{V}\nabla^{\xi}\cdot(J\boldsymbol{J}^{-1}\mathcal{F})\text{d}V
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\nabla^{\xi}$
\end_inset

为局部坐标系下的Hamiltion算符．
\end_layout

\begin_layout Standard
利用Reynolds输运方程
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\int_{v(t)}\frac{\partial\boldsymbol{U}}{\partial t}\text{d}v & = & \frac{\text{d}}{\text{d}t}\int_{v(t)}\boldsymbol{U}\text{d}v-\int_{s(t)}\boldsymbol{U}\boldsymbol{v}_{g}\cdot\text{d}\boldsymbol{s}\\
 & = & \frac{\text{d}}{\text{d}t}\int_{V}J\boldsymbol{U}\text{d}v-\int_{S}J\boldsymbol{J}^{-1}\boldsymbol{U}\boldsymbol{v}_{g}\cdot\text{d}\boldsymbol{s}\\
 & = & \int_{V}\frac{\partial(J\boldsymbol{U})}{\partial t}\text{d}v-\int_{S}J\boldsymbol{J}^{-1}\boldsymbol{U}\boldsymbol{v}_{g}\cdot\text{d}\boldsymbol{s}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
可得
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{t}(J\boldsymbol{U})+\nabla^{\xi}\cdot\left(J\boldsymbol{J}^{-1}(\mathcal{F}-\boldsymbol{U}\boldsymbol{v}_{g})\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard
如果计算网格不随时间运动，网格运动速度
\begin_inset Formula $\boldsymbol{v}_{g}=0$
\end_inset

，则
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\partial_{t}(J\boldsymbol{U})+\nabla^{\xi}\cdot\left(J\boldsymbol{J}^{-1}\mathcal{F}\right)=0\label{eq:local NS eqn}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
上式即为局部坐标系下的的流体力学控制方程．
\end_layout

\begin_layout Standard
N-S方程
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\partial_{t}\boldsymbol{u}+\nabla\cdot\mathcal{F}^{c}(\boldsymbol{u})-\nabla\cdot\mathcal{F}^{v}(\boldsymbol{u},\nabla\boldsymbol{u})=0\label{eq:spacial1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
局部坐标系
\begin_inset Formula $\boldsymbol{\xi}$
\end_inset

下，写为
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\partial_{t}(J\boldsymbol{u})+\nabla^{\xi}\cdot\left(J\boldsymbol{J}^{-1}(\mathcal{F}^{c}-\mathcal{F}^{v}-\boldsymbol{u}\boldsymbol{v}_{g})\right)=0\label{eq:spacial2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
其中
\begin_inset Formula $\nabla^{\xi}$
\end_inset

为坐标系
\begin_inset Formula $\boldsymbol{\xi}$
\end_inset

下的Hamilton算符，且有
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\nabla=\boldsymbol{J}^{-1}\nabla^{\xi}
\]

\end_inset


\end_layout

\begin_layout Standard
则
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:spacial2"

\end_inset

式可写为
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{t}(J\boldsymbol{u})+\nabla\cdot\left(J(\mathcal{F}^{c}-\mathcal{F}^{v}-\boldsymbol{u}\boldsymbol{v}_{g})\right)=0
\]

\end_inset


\end_layout

