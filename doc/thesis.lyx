\end_layout

\begin_layout Standard
远场的法向量的特征矩阵
\begin_inset Formula $A(\boldsymbol{u},\boldsymbol{n})=\frac{\partial f_{p\alpha}^{c}}{\partial u_{q}}n_{\alpha}$
\end_inset

的特征值
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lambda_{1}=\boldsymbol{v}\cdot\boldsymbol{n}-c,\ \lambda_{2,3,4}=\boldsymbol{v}\cdot\boldsymbol{n},\ \lambda_{5}=\boldsymbol{v}\cdot\boldsymbol{n}+c
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $c=\sqrt{\gamma p/\rho}$
\end_inset

表示声速．跟不
\begin_inset Formula $\lambda_{i}$
\end_inset

的符号，远场可建立四种边界条件：
\end_layout

\begin_layout Itemize
超声速入口：
\begin_inset Formula $\lambda_{i}<0,i=1,\cdots,5$
\end_inset

，
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{u}^{-}(\boldsymbol{u}^{+})=\boldsymbol{u}_{\infty}
\]

\end_inset


\end_layout

\begin_layout Itemize
亚声速入口：
\begin_inset Formula $\lambda_{i}<0,i=1,\cdots,4,\lambda_{5}>0$
\end_inset

，
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\rho^{-} & = & \rho_{\infty}\\
(\rho\boldsymbol{v})^{-} & = & (\rho\boldsymbol{v})_{\infty}\\
p^{-} & = & p^{+}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
亚声速出口：
\begin_inset Formula $\lambda_{\text{１}}<0,\lambda_{i}\text{＞}0,i=\text{２},\cdots,5$
\end_inset

，
\end_layout

\begin_layout Standard
亚声速出口指定出口压强，其他值由流场插值而来．
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\rho^{-} & = & \rho^{+}\\
(\rho\boldsymbol{v})^{-} & = & (\rho\boldsymbol{v})^{+}\\
p^{-} & = & p_{\text{out}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $p_{\text{out}}=p_{\infty}$
\end_inset

.
\end_layout

\begin_layout Itemize
超声速出口：
\begin_inset Formula $\lambda_{i}\text{＞}0,i=1,\cdots,5$
\end_inset

．
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{u}^{-}(\boldsymbol{u}^{+})=\boldsymbol{u}^{+}
\]

\end_inset


\end_layout

\begin_layout Standard
无粘物面边界的速度为无穿透条件，数学提法为：
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{v}\cdot\boldsymbol{n}=0
\]

\end_inset


\end_layout

\begin_layout Standard
几何描述如图。
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{v}^{-}=\boldsymbol{v}^{+}-2\boldsymbol{v}_{n}
\]

\end_inset


\end_layout

\begin_layout Standard
而
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{v}_{n}=(\boldsymbol{v}\cdot\boldsymbol{n})\boldsymbol{n}
\]

\end_inset


\end_layout

\begin_layout Standard
即
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{v}^{-}=\boldsymbol{v}^{+}-2(\boldsymbol{v}\cdot\boldsymbol{n})\boldsymbol{n}
\]

\end_inset


\end_layout

\begin_layout Standard
缺少的另外两个边界补充为
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\rho^{-} & = & \rho^{+}\\
p^{-} & = & p^{+}
\end{eqnarray*}

\end_inset


\end_layout

