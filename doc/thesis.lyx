\begin_layout Standard
时间BDF1离散
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\kappa}\text{\ensuremath{\partial}}_{t}\boldsymbol{u}\cdot\boldsymbol{\varphi}\text{d}\Omega\approx\int_{\kappa}\frac{u_{p}-u_{p}^{t}}{\Delta t}\varphi_{i}\text{d}\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\text{d}}{\text{d}u_{qj}}\int_{\kappa}\frac{u_{p}-u_{p}^{t}}{\Delta t}\varphi_{i}\text{d}\Omega=\frac{1}{\Delta t}\int_{\kappa}\frac{\partial u_{p}}{\partial u_{qj}}\phi_{i}\text{d}\Omega=\frac{1}{\Delta t}\int_{\kappa}\delta_{pq}\phi_{j}\varphi_{i}\text{d}\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
上式用到
\end_layout

\begin_layout Standard
由于
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
u_{q}=u_{qj}\phi_{j}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial u_{p}}{\partial u_{qj}}=\delta_{pq}\phi_{j}
\]

\end_inset


\end_layout

\begin_layout Subsection
体积分的Jacobian矩阵
\end_layout

\begin_layout Standard
体积分
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\int_{\kappa}(\mathcal{F}^{c}-\mathcal{F}^{v}):\nabla\boldsymbol{\varphi}\text{d}\Omega=-\int_{\kappa}f_{p\alpha}\partial_{\alpha}\varphi_{i}\text{d}\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
对变量自由度的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\text{d}}{\text{d}u_{qj}}\int_{\kappa}f_{p\alpha}\partial_{\alpha}\varphi_{i}\text{d}\Omega=\int_{\kappa}\frac{\text{d}f_{p\alpha}}{\text{d}u_{qj}}\partial_{\alpha}\varphi_{i}\text{d}\Omega=\int_{\kappa}\left(\frac{\partial f_{p\alpha}}{\partial u_{q}}\frac{\partial u_{q}}{\partial u_{qj}}+\frac{\partial f_{p\alpha}}{\partial(\partial_{\beta}u_{q})}\frac{\partial(\partial_{\beta}u_{q})}{\partial u_{qj}}\right)\partial_{\alpha}\varphi_{i}\text{d}\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
注意到
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial u_{q}}{\partial u_{qj}}=\phi_{j}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial(\partial_{\beta}u_{q})}{\partial u_{qj}}=\partial_{\beta}\phi_{j}
\]

\end_inset


\end_layout

\begin_layout Standard
则
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\text{d}}{\text{d}u_{qj}}\int_{\kappa}f_{p\alpha}\partial_{\alpha}\varphi_{i}\text{d}\Omega=\int_{\kappa}\left(\frac{\partial f_{p\alpha}}{\partial u_{q}}\phi_{j}+\frac{\partial f_{p\alpha}}{\partial(\partial_{\beta}u_{q})}\partial_{\beta}\phi_{j}\right)\partial_{\alpha}\varphi_{i}\text{d}\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
用有限差分方法计算
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f_{p,\alpha}}{\partial u_{q}}\approx\frac{f_{p\alpha}(u_{q}+\varepsilon,\partial_{\beta}u_{q})-f_{p\alpha}(u_{q},\partial_{\beta}u_{q})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f_{p,\alpha}}{\partial(\partial_{\beta}u_{q})}\approx\frac{f_{p\alpha}(u_{q},\partial_{\beta}u_{q}+\varepsilon)-f_{p\alpha}(u_{q},\partial_{\beta}u_{q})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Subsection
通量积分的Jacobian矩阵
\end_layout

\begin_layout Standard
面上有正、负通量，依赖与左、右值。Jacobian矩阵有四种组合.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\partial\kappa}(\hat{\mathcal{F}^{c}}-\hat{\mathcal{F}^{v}}\boldsymbol{n})\cdot\boldsymbol{\varphi}^{+}\text{d}S=\int_{\partial\kappa}h_{p}\varphi_{i}^{+}\text{d}s
\]

\end_inset


\end_layout

\begin_layout Standard
面的负通量
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-\int_{\partial\kappa}(\hat{\mathcal{F}^{c}}-\hat{\mathcal{F}^{v}}\boldsymbol{n})\cdot\boldsymbol{\varphi}^{-}\text{d}S=\int_{\partial\kappa}h_{p}\varphi_{i}^{-}\text{d}s
\]

\end_inset


\end_layout

\begin_layout Subsubsection
正通量对左值，ElementElement(+, +)的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{\text{d}}{\text{d}u_{qj}^{+}}\int_{\partial\kappa}h_{p}\varphi_{i}^{+}\text{d}s & = & \int_{\partial\kappa}\frac{\text{d}h_{p}}{\text{d}u_{qj}^{+}}\varphi_{i}^{+}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{+}}\frac{\partial u_{q}^{+}}{\partial u_{qj}^{+}}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{+})}\frac{\partial(\partial_{\beta}u_{q}^{+})}{\partial u_{qj}^{+}}\right)\varphi_{i}^{+}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{+}}\phi_{j}^{+}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{+})}\partial_{\beta}\phi_{j}^{+}\right)\varphi_{i}^{+}\text{d}s
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsubsection
正通量对右值，ElementNeighbor(+, -)的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{\text{d}}{\text{d}u_{qj}^{-}}\int_{\partial\kappa}h_{p}\varphi_{i}^{+}\text{d}s & = & \int_{\partial\kappa}\frac{\text{d}h_{p}}{\text{d}u_{qj}^{-}}\varphi_{i}^{+}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{-}}\frac{\partial u_{q}^{-}}{\partial u_{qj}^{-}}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{-})}\frac{\partial(\partial_{\beta}u_{q}^{-})}{\partial u_{qj}^{-}}\right)\varphi_{i}^{+}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{-}}\phi_{j}^{-}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{-})}\partial_{\beta}\phi_{j}^{-}\right)\varphi_{i}^{+}\text{d}s
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsubsection
负通量对左值，NeighborElement(-, +)的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
-\frac{\text{d}}{\text{d}u_{qj}^{+}}\int_{\partial\kappa}h_{p}\varphi_{i}^{-}\text{d}s & = & -\int_{\partial\kappa}\frac{\text{d}h_{p}}{\text{d}u_{qj}^{+}}\varphi_{i}^{-}\text{d}s\\
 & = & -\int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{+}}\frac{\partial u_{q}^{+}}{\partial u_{qj}^{+}}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{+})}\frac{\partial(\partial_{\beta}u_{q}^{+})}{\partial u_{qj}^{+}}\right)\varphi_{i}^{-}\text{d}s\\
 & =- & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{+}}\phi_{j}^{+}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{+})}\partial_{\beta}\phi_{j}^{+}\right)\varphi_{i}^{-}\text{d}s
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsubsection
负通量对右值，NeighborNeighbor(-, -)的Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
-\frac{\text{d}}{\text{d}u_{qj}^{-}}\int_{\partial\kappa}h_{p}\varphi_{i}^{-}\text{d}s & = & \int_{\partial\kappa}\frac{\text{d}h_{p}}{\text{d}u_{qj}^{-}}\varphi_{i}^{-}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{-}}\frac{\partial u_{q}^{-}}{\partial u_{qj}^{-}}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{-})}\frac{\partial(\partial_{\beta}u_{q}^{-})}{\partial u_{qj}^{-}}\right)\varphi_{i}^{-}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial h_{p}}{\partial u_{q}^{-}}\phi_{j}^{-}+\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{-})}\partial_{\beta}\phi_{j}^{-}\right)\varphi_{i}^{-}\text{d}s
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsubsection
有限差分计算Jacobian矩阵
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial h_{p}}{\partial u_{q}^{+}}\approx\frac{h_{p}(u_{q}^{+}+\varepsilon,u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})-h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{+})}\approx\frac{h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+}+\varepsilon,\partial_{\beta}u_{q}^{-})-h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial h_{p}}{\partial u_{q}^{-}}\approx\frac{h_{p}(u_{q}^{+},u_{q}^{-}+\varepsilon,\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})-h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial h_{p}}{\partial(\partial_{\beta}u_{q}^{-})}\approx\frac{h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-}+\varepsilon)-h_{p}(u_{q}^{+},u_{q}^{-},\partial_{\beta}u_{q}^{+},\partial_{\beta}u_{q}^{-})}{\varepsilon}
\]

\end_inset


\end_layout

\begin_layout Subsection
罚积分的Jacobian矩阵
\end_layout

\begin_layout Standard
正罚积分
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\partial\kappa}\mathcal{F}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}]):\nabla\boldsymbol{\varphi}\text{d}s=\int_{\partial\kappa}\mathcal{F}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}]):\nabla\boldsymbol{\varphi}^{+}\text{d}s
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{\text{d}}{\text{d}u_{qj}^{+}}\int_{\partial\kappa}f_{p\alpha}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}])\partial_{\alpha}\varphi_{i}^{+}\text{d}s & = & \int_{\partial\kappa}\frac{\text{d}f_{p\alpha}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}])}{\text{d}u_{qj}^{+}}\varphi_{i}\text{d}s\\
 & = & \int_{\partial\kappa}\left(\frac{\partial f_{p\alpha}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}])}{\partial u_{q}^{+}}\frac{\partial u_{q}^{+}}{\partial u_{qj}^{+}}\right)\varphi_{i}\text{d}s\\
 & = & \int_{\partial\kappa}\frac{\partial f_{p\alpha}^{v}(\boldsymbol{u}^{+},\frac{1}{2}[\boldsymbol{u}])}{\partial u_{q}^{+}}\phi_{j}^{+}\varphi_{i}^{-}\text{d}s
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
