\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\rho^{-} & = & \rho^{+}\\
(\rho\boldsymbol{v})^{-} & = & (\rho\boldsymbol{v})^{+}\\
p^{-} & = & p_{\text{out}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $p_{\text{out}}=p_{\infty}$
\end_inset

.
\end_layout

\begin_layout Itemize
